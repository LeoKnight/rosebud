<view class="cml-checkbox  cml-base cml-view cml-dd4981bc">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-dd4981bc" onTap="_cmlEventProxy" data-eventtap="changeCheck">
    <view class="cml-checkbox-check  cml-base cml-view cml-dd4981bc" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-dd4981bc">
        <image class="inner-icon  cml-base cml-image cml-dd4981bc" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-dd4981bc">{{label}}</text>
  </view>
</view>