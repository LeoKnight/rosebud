        <demo-com title="{{title}}" image-src="{{chameleonSrc}}" class=" cml-view cml-demo-com"></demo-com>
      </view>
      <view class=" cml-base cml-view">
        <cml-buildin-button type="blue" text="开始我们的旅程" bindonclick="_cmlEventProxy" data-eventonclick="testclick" class=" cml-base cml-button">
        </cml-buildin-button>
      </view>
    </cml-buildin-scroller>
</cml-buildin-page>